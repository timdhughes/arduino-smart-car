{
    "configurations": [
        {
            "name": "Win32",
            "defines": [
                "ARDUINO=10800"
            ],
            "includePath": [
                "${workspaceRoot}",
                "${arduinoRoot}/hardware/arduino/avr/**",
                "${arduinoRoot}/tools/**",
                "${arduinoRoot}/hardware/tools/**",
                "${arduinoRoot}/libraries/**"
            ],
            "forcedInclude": [
                "${arduinoRoot}/hardware/arduino/avr/cores/arduino/Arduino.h"
            ],
            "intelliSenseMode": "clang-x64",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}